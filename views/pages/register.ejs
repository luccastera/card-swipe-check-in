<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

    <title>Card Swipe Check In</title>
    <meta name="description" content="Check in with a card swipe">
    <meta name="keywords" content="MiamiJS">
    <meta name="author" content="Luc Castera">

    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/css/screen.css">
  </head>
  <body id="">
    <div class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <a href="/" class="navbar-brand">Card Swipe Check In</a>
        </div>
        <div class="navbar-collapse collapse" id="navbar-main">
        </div>
      </div>
    </div>
    <div id="main-wrapper">
      <div class="container">
        <div class="row" id="new-maker-form">
          <div class="col-xs-12 col-md-6">
            <h2>Register a new user:</h2>
            <br />
            <form action="/register" method="POST">
              <div class="form-group">
                <label for="name">Card ID:</label>
                <input id="card_id" name="card_id" type="text" class="form-control" />
              </div>
              <div class="form-group">
                <label for="firstname">First Name:</label>
                <input name="firstname" type="text" class="form-control" />
              </div>
              <div class="form-group">
                <label for="lastname">Last Name:</label>
                <input name="lastname" type="text" class="form-control" />
              </div>
              <div class="form-group">
                <label for="city">City:</label>
                <input name="city" type="text" class="form-control" />
              </div>
              <div class="form-group">
                <label for="state">State:</label>
                <input name="state" type="text" class="form-control" />
              </div>
              <div class="form-group">
                <input type="submit" class="btn btn-primary" value="Register" />
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <footer class="container footer">
      <p class="text-muted">
        Built at MiamiJS meetup.
      </p>
    </footer>
  </body>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
  <script src="/js/moment.min.js"></script>
  <script src="/js/app.js"></script>
</html


